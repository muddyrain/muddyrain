{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "jsx": "react",
    "baseUrl": "./",
    "moduleResolution":"node",
    "noImplicitThis": false,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "muddyrain": ["src"],
      "@^/*": ["./*"],
      "muddyrain/*": ["src/*", "*"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*", "src/hooks/useScrollTopBottom.tsx"]
}
